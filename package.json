{
  "name": "jeoparody",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "run-p dev:* open-browser",
    "dev:next": "next dev",
    "dev:wss": "cross-env NODE_ENV=development tsx watch src/server/trpc/wsServer/wsDevServer.ts",
    "build": "run-s build:*",
    "build:next": "cross-env NODE_ENV=production next build",
    "build:server": "ttsc --project tsconfig.server.json",
    "start": "cross-env NODE_ENV=production node dist/server/trpc/wsServer/wsProdServer.js",
    "lint": "next lint",
    "open-browser": "start http://localhost:3000",
    "eslint": "eslint ./src --fix",
    "prepare": "ts-patch install -s"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/server": "^11.10.0",
    "@mantine/core": "^5.10.2",
    "@mantine/hooks": "^5.10.2",
    "@mantine/next": "^5.10.2",
    "@tabler/icons": "^2.1.2",
    "@tabler/icons-react": "^2.1.2",
    "@tanstack/react-query": "^4.24.4",
    "@trpc/client": "^10.9.1",
    "@trpc/next": "^10.9.1",
    "@trpc/react-query": "^10.9.1",
    "@trpc/server": "^10.9.1",
    "next": "13.1.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "superjson": "1.12.2",
    "ws": "^8.12.0",
    "zod": "^3.20.2",
    "zustand": "^4.3.2"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@types/ws": "^8.5.4",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.33.0",
    "eslint-config-next": "13.1.6",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "npm-run-all": "^4.1.5",
    "ts-patch": "^2.1.0",
    "tsx": "^3.12.2",
    "ttypescript": "^1.5.15",
    "typescript": "^4.9.5",
    "typescript-transform-paths": "^3.4.6"
  }
}
